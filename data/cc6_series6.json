{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Unemployment Rate",
    "fontSize": 12
  },
  "width": "container",
  "height": 250,
  "data": {
    "url": "https://raw.githubusercontent.com/gabrielcostapublicpolicy/gabrielcostapublicpolicy.github.io/main/cc6_data6.json"
  },
  "mark": {
    "type": "line",
    "strokeWidth": 2,
    "color": "#e67e22"
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "title": null,
      "axis": {
        "format": "%Y"
      }
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "title": "Rate (%)",
      "scale": {
        "domain": [0, 15]
      }
    },
    "tooltip": [
      {
        "field": "date",
        "type": "temporal",
        "format": "%Y-%m",
        "title": "Date"
      },
      {
        "field": "value",
        "type": "quantitative",
        "format": ".1f",
        "title": "Unemployment (%)"
      }
    ]
  }
}