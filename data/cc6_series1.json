{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {"text": "Women's Labor Force Participation", "fontSize": 12},
  "width": "container",
  "height": 250,

  "data": {
  "url": "data/cc6_data1.json"
}
,

  "mark": {"type": "line", "strokeWidth": 2},
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {"format": "%Y"}
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "title": "Rate (%)"
    },
    "tooltip": [
      {"field": "date", "type": "temporal", "title": "Date", "format": "%Y-%m"},
      {"field": "value", "type": "quantitative", "title": "Rate (%)", "format": ".1f"}
    ]
  }
}
